  <ItemGroup>
    <PackageReference Include="FluentAssertions" Version="5.0.0-beta0004" />
    <PackageReference Include="NUnit" Version="3.9.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\lib\lib.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="bosses\**" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="bosses\**" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="bosses\**" />
  </ItemGroup>
</Project>