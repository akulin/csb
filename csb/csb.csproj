    <OutputType>Exe</OutputType>
    <TargetFramework>net471</TargetFramework>
    <LangVersion>7</LangVersion>
    <RootNamespace>CG.CodersStrikeBack</RootNamespace>
    <OutputPath>bin</OutputPath>
